def partie5(file):
    linked_list_fichier = []
    f = open(file, "r")
    if not f: print("File not found"); exit(0)
    for line in f.readlines():
        linked_list_fichier.append(list(map(int, line.split())))
    f.close()
    return linked_list_fichier


l = partie5("Input.txt")                    #Pour tester
print(l)          #Pour tester
print(l[0])       #Pour tester
print(l[0][0])    #Pour tester
